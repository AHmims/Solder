{
  "name": "Electroplanet",
  "description": "Electroplanet is a moroccan company specialised in selling different products such as appliances, TV, Hifi and Multimedia.",
  "baseUrl": "https://www.electroplanet.ma",
  "config": {
    "productPage": {
      "scraper": "SSR",
